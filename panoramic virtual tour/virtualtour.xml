<krpano version="1.0.8.14" logkey="false">
    <action name="mainloadpano">
		closepanoobjects();
		ifnot(stopSequence === undefined,interruptAnimation(););
		loadpano(%1,NULL,NULL,BLEND(1));
	</action>
    <!--************** KrPano plugins and datas ***************************************************-->
