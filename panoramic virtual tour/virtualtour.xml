    <!--************** Introduction ***************************************************************-->
    <action name="firstloadcompleteaction">
		if(plugin[data].firstPanoHasBeenPlayed==false,
			set(plugin[data].firstPanoHasBeenPlayed,true);
			set(autorotate.enabled,true);
		);
	</action>
    <!--************** Menu ***********************************************************************-->
